{
  "name": "mdb-electron",
  "version": "0.0.0",
  "main": "main.js",
  "productName": "mdb",
  "private": true,
  "engines": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "\n# ELECTRON SCRIPTS": "",
    "electron": "electron .",
    "electron-logging": "electron . --enable-logging",
    "electron2": "electron",
    "electron-build2": "ng build --prod && npm run electron",
    "electron-build-d": "ng build --dev && npm run electron",
    "electron-build": "ng build --prod && electron .",
    "start:electron": "ng build --base-href ./ && electron .",
    "start:electron-prod": "ng build --base-href ./ --prod --aot && electron .",
    "\n# STARTING ANGULAR AND NODE SERVER": "",
    "start:server": "node src/assets/scripts/server/main",
    "start:angular-server": " start ng serve & start npm run start:server",
    "start:angular-server2": "start npm start && npm run start:server",
    "pack": "electron-packager ./ --overwrite",
    "clean:install": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && install --save --save-exact",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
