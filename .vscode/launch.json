{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
  {
    "type": "java",
    "request": "attach",
    "name": "Attach by Process ID",
    "processId": "${command:PickJavaProcess}"
  },
  {
    "name": "Launch Edge",
    "request": "launch",
    "type": "pwa-msedge",
    "url": "http://localhost:8080",
    "webRoot": "${workspaceFolder}"
  },
  {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\main.js",
      // "program": "${workspaceFolder}\\async.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
      "program": "${workspaceRoot}/main.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Script",
      "program": "${workspaceFolder}\\src\\assets\\scripts\\bookmarks-service.js",
      // "program": "${workspaceFolder}\\async.ts",
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Watched db service",
      "program": "${workspaceFolder}\\src\\assets\\scripts\\watched-db-service.js",
      // "program": "${workspaceFolder}\\async.ts",
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "scan-library",
      "program": "${workspaceFolder}\\src\\assets\\scripts\\scan-library.js",
      // "program": "${workspaceFolder}\\async.ts",
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "pre-start",
      "program": "${workspaceFolder}\\src\\assets\\scripts\\pre-start.js",
      // "program": "${workspaceFolder}\\async.ts",
      "outFiles": [
        "${workspaceFolder}/dist/out-tsc/**/*.js"
      ]
    },
  ]
}
